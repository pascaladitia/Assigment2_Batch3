<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.assigment2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.main.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.listView.ListViewActivity" />
        <activity android:name=".view.umur.UmurActivity" />
        <activity android:name=".view.recyclerview.RecyclerviewActivity" />
        <activity android:name=".view.calculator.CalculatorActivity" />
        <activity android:name=".view.main.DashboardActivity" />
        <activity android:name=".view.main.MainActivity"/>
    </application>

</manifest>